79056>>>>>>>>>>>>>    // parse passed format string and set temporary properties with result
79056>>>>>>>>>>>>>    //
79056>>>>>>>>>>>>>    Procedure ParseFormat string sFmt
79058>>>>>>>>>>>>>
79058>>>>>>>>>>>>>        string sLeft sRight sDigit
79058>>>>>>>>>>>>>        integer bSep iPos i iDigits
79058>>>>>>>>>>>>>
79058>>>>>>>>>>>>>        // replace any literals. A "/" followed by anything.
79058>>>>>>>>>>>>>        // some literals are special. $ . , / #
79058>>>>>>>>>>>>>        Move (Replaces("/$",sFmt,Character(1)))    to sFmt
79059>>>>>>>>>>>>>        Move (Replaces("/.",sFmt,Character(2)))    to sFmt
79060>>>>>>>>>>>>>        Move (Replaces("/,",sFmt,Character(3)))    to sFmt
79061>>>>>>>>>>>>>        Move (Replaces("/"+"/",sFmt,Character(4))) to sFmt
79062>>>>>>>>>>>>>        Move (Replaces("/#",sFmt,Character(5)))    to sFmt
79063>>>>>>>>>>>>>        Move (Character(9)) to sDigit
79064>>>>>>>>>>>>>        Move (Replaces("#",sFmt,sDigit))           to sFmt
79065>>>>>>>>>>>>>        Move (Replaces("/",sFmt,""))               to sFmt // replace all others
79066>>>>>>>>>>>>>
79066>>>>>>>>>>>>>        Move (Pos(",",sFmt))                 to bSep // if we have any , we use thousand seps
79067>>>>>>>>>>>>>        If bSep Move (Replaces(",",sFmt,"")) to sFmt // remove all ,
79070>>>>>>>>>>>>>
79070>>>>>>>>>>>>>        Move (Replaces("$",sFmt,psCurrencySymbol(self))) to sFmt // replace any $ with currency symbol
79071>>>>>>>>>>>>>
79071>>>>>>>>>>>>>        Move (Pos(".",sFmt)) to iPos                 // position of decimal
79072>>>>>>>>>>>>>
79072>>>>>>>>>>>>>        // Move all the special literals back into place before parsing
79072>>>>>>>>>>>>>        Move (Replaces(Character(1),sFmt,"$")) to sFmt
79073>>>>>>>>>>>>>        Move (Replaces(Character(2),sFmt,".")) to sFmt
79074>>>>>>>>>>>>>        Move (Replaces(Character(3),sFmt,",")) to sFmt
79075>>>>>>>>>>>>>        Move (Replaces(Character(4),sFmt,"/")) to sFmt
79076>>>>>>>>>>>>>        Move (Replaces(Character(5),sFmt,"#")) to sFmt
79077>>>>>>>>>>>>>
79077>>>>>>>>>>>>>        If (iPos>0) Begin                      // if we have a decimanl point
79079>>>>>>>>>>>>>            Move 1 to i                        // look for first non # to right and count the #s
79080>>>>>>>>>>>>>            If (mid(sFmt,1,iPos+i)="*") Begin  // the "*" is special. It means as many as you want
79082>>>>>>>>>>>>>               Move -2 to iDigits
79083>>>>>>>>>>>>>               increment i
79084>>>>>>>>>>>>>            end
79084>>>>>>>>>>>>>>
79084>>>>>>>>>>>>>            While (mid(sFmt,1,iPos+i)=sDigit)
79088>>>>>>>>>>>>>                 Increment i
79089>>>>>>>>>>>>>            end
79090>>>>>>>>>>>>>>
79090>>>>>>>>>>>>>            Move (Mid(sFmt,255,iPos+i)) to sRight // everything to the right of the last # is format stuff
79091>>>>>>>>>>>>>            If (iDigits=0) Move (i-1) to iDigits
79094>>>>>>>>>>>>>            //
79094>>>>>>>>>>>>>            Move 1 to i                           // find the first non-# to the left of the point
79095>>>>>>>>>>>>>            While (mid(sFmt,1,iPos-i)=sDigit)     // everything to the left is format stuff
79099>>>>>>>>>>>>>                 increment i
79100>>>>>>>>>>>>>            end
79101>>>>>>>>>>>>>>
79101>>>>>>>>>>>>>            Move (left(sFmt,iPos-i)) to sLeft
79102>>>>>>>>>>>>>        end
79102>>>>>>>>>>>>>>
79102>>>>>>>>>>>>>        else begin                             // we have no decinal
79103>>>>>>>>>>>>>            Move 0 to iDigits                  // so points is none
79104>>>>>>>>>>>>>            Move (Pos(sDigit,sFmt)) to iPos    // find first #.
79105>>>>>>>>>>>>>            If (iPos=0) Begin                  // if none, entire string is left format stuff..wierd!
79107>>>>>>>>>>>>>                Move sFmt to sLeft
79108>>>>>>>>>>>>>                Move ""   to sRight
79109>>>>>>>>>>>>>            end
79109>>>>>>>>>>>>>>
79109>>>>>>>>>>>>>            else begin
79110>>>>>>>>>>>>>                Move (left(sFmt,iPos-1)) to sLeft // all char to left of first # is left format stuff
79111>>>>>>>>>>>>>                Move 1 to i
79112>>>>>>>>>>>>>                While (mid(sFmt,1,iPos+i)=sDigit) // find last #, all char to right is right format
79116>>>>>>>>>>>>>                    Increment i
79117>>>>>>>>>>>>>                end
79118>>>>>>>>>>>>>>
79118>>>>>>>>>>>>>                Move (Mid(sFmt,255,i+iPos)) to sRight
79119>>>>>>>>>>>>>            end
79119>>>>>>>>>>>>>>
79119>>>>>>>>>>>>>        end
79119>>>>>>>>>>>>>>
79119>>>>>>>>>>>>>        // set temporary format properties and exit
79119>>>>>>>>>>>>>        Set pbThousandsSep to bSep
79120>>>>>>>>>>>>>        set psLeft         to sLeft
79121>>>>>>>>>>>>>        set psRight        to sRight
79122>>>>>>>>>>>>>        Set piPoints       to iDigits
