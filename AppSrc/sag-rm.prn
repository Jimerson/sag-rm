98087>>>>>>>        String sNewVal sHold
98087>>>>>>>        If (Not(Pos('"', sVal))) Function_Return ('"'+sVal+'"')
98090>>>>>>>        If (Not(Pos("'", sVal))) Function_Return ("'"+sVal+"'")
98093>>>>>>>        Move '"' To sNewVal
98094>>>>>>>        While (Pos('"', sVal))
98098>>>>>>>            Move (Left(sVal, (Pos('"', sVal)-1))) To sHold
98099>>>>>>>            Move (Replace((sHold+'"'), sVal, "")) To sVal
98100>>>>>>>            If (sNewVal <> '"') Append sNewVal '+"'
98103>>>>>>>            Append sNewVal sHold '"' "+'" '"' "'"
98108>>>>>>>        Loop
98109>>>>>>>>
98109>>>>>>>        If (Length(sVal)) Append sNewVal '+"' sVal '"'
98114>>>>>>>        Function_Return sNewVal
