79065>>>>>>>>>>>>>    // internal
79065>>>>>>>>>>>>>    // parse passed format string and set temporary properties with result
79065>>>>>>>>>>>>>    //
79065>>>>>>>>>>>>>    Procedure ParseFormat string sFmt
79067>>>>>>>>>>>>>
79067>>>>>>>>>>>>>        string sLeft sRight sDigit
79067>>>>>>>>>>>>>        integer bSep iPos i iDigits
79067>>>>>>>>>>>>>
79067>>>>>>>>>>>>>        // replace any literals. A "/" followed by anything.
79067>>>>>>>>>>>>>        // some literals are special. $ . , / #
79067>>>>>>>>>>>>>        Move (Replaces("/$",sFmt,Character(1)))    to sFmt
79068>>>>>>>>>>>>>        Move (Replaces("/.",sFmt,Character(2)))    to sFmt
79069>>>>>>>>>>>>>        Move (Replaces("/,",sFmt,Character(3)))    to sFmt
79070>>>>>>>>>>>>>        Move (Replaces("/"+"/",sFmt,Character(4))) to sFmt
79071>>>>>>>>>>>>>        Move (Replaces("/#",sFmt,Character(5)))    to sFmt
79072>>>>>>>>>>>>>        Move (Character(9)) to sDigit
79073>>>>>>>>>>>>>        Move (Replaces("#",sFmt,sDigit))           to sFmt
79074>>>>>>>>>>>>>        Move (Replaces("/",sFmt,""))               to sFmt // replace all others
79075>>>>>>>>>>>>>
79075>>>>>>>>>>>>>        Move (Pos(",",sFmt))                 to bSep // if we have any , we use thousand seps
79076>>>>>>>>>>>>>        If bSep Move (Replaces(",",sFmt,"")) to sFmt // remove all ,
79079>>>>>>>>>>>>>
79079>>>>>>>>>>>>>        Move (Replaces("$",sFmt,psCurrencySymbol(self))) to sFmt // replace any $ with currency symbol
79080>>>>>>>>>>>>>
79080>>>>>>>>>>>>>        Move (Pos(".",sFmt)) to iPos                 // position of decimal
79081>>>>>>>>>>>>>
79081>>>>>>>>>>>>>        // Move all the special literals back into place before parsing
79081>>>>>>>>>>>>>        Move (Replaces(Character(1),sFmt,"$")) to sFmt
79082>>>>>>>>>>>>>        Move (Replaces(Character(2),sFmt,".")) to sFmt
79083>>>>>>>>>>>>>        Move (Replaces(Character(3),sFmt,",")) to sFmt
79084>>>>>>>>>>>>>        Move (Replaces(Character(4),sFmt,"/")) to sFmt
79085>>>>>>>>>>>>>        Move (Replaces(Character(5),sFmt,"#")) to sFmt
79086>>>>>>>>>>>>>
79086>>>>>>>>>>>>>        If (iPos>0) Begin                      // if we have a decimanl point
79088>>>>>>>>>>>>>            Move 1 to i                        // look for first non # to right and count the #s
79089>>>>>>>>>>>>>            If (mid(sFmt,1,iPos+i)="*") Begin  // the "*" is special. It means as many as you want
79091>>>>>>>>>>>>>               Move -2 to iDigits
79092>>>>>>>>>>>>>               increment i
79093>>>>>>>>>>>>>            end
79093>>>>>>>>>>>>>>
79093>>>>>>>>>>>>>            While (mid(sFmt,1,iPos+i)=sDigit)
79097>>>>>>>>>>>>>                 Increment i
79098>>>>>>>>>>>>>            end
79099>>>>>>>>>>>>>>
79099>>>>>>>>>>>>>            Move (Mid(sFmt,255,iPos+i)) to sRight // everything to the right of the last # is format stuff
79100>>>>>>>>>>>>>            If (iDigits=0) Move (i-1) to iDigits
79103>>>>>>>>>>>>>            //
79103>>>>>>>>>>>>>            Move 1 to i                           // find the first non-# to the left of the point
79104>>>>>>>>>>>>>            While (mid(sFmt,1,iPos-i)=sDigit)     // everything to the left is format stuff
79108>>>>>>>>>>>>>                 increment i
79109>>>>>>>>>>>>>            end
79110>>>>>>>>>>>>>>
79110>>>>>>>>>>>>>            Move (left(sFmt,iPos-i)) to sLeft
79111>>>>>>>>>>>>>        end
79111>>>>>>>>>>>>>>
79111>>>>>>>>>>>>>        else begin                             // we have no decinal
79112>>>>>>>>>>>>>            Move 0 to iDigits                  // so points is none
79113>>>>>>>>>>>>>            Move (Pos(sDigit,sFmt)) to iPos    // find first #.
79114>>>>>>>>>>>>>            If (iPos=0) Begin                  // if none, entire string is left format stuff..wierd!
79116>>>>>>>>>>>>>                Move sFmt to sLeft
79117>>>>>>>>>>>>>                Move ""   to sRight
79118>>>>>>>>>>>>>            end
79118>>>>>>>>>>>>>>
79118>>>>>>>>>>>>>            else begin
79119>>>>>>>>>>>>>                Move (left(sFmt,iPos-1)) to sLeft // all char to left of first # is left format stuff
79120>>>>>>>>>>>>>                Move 1 to i
79121>>>>>>>>>>>>>                While (mid(sFmt,1,iPos+i)=sDigit) // find last #, all char to right is right format
79125>>>>>>>>>>>>>                    Increment i
79126>>>>>>>>>>>>>                end
79127>>>>>>>>>>>>>>
79127>>>>>>>>>>>>>                Move (Mid(sFmt,255,i+iPos)) to sRight
79128>>>>>>>>>>>>>            end
79128>>>>>>>>>>>>>>
79128>>>>>>>>>>>>>        end
79128>>>>>>>>>>>>>>
79128>>>>>>>>>>>>>        // set temporary format properties and exit
79128>>>>>>>>>>>>>        Set pbThousandsSep to bSep
79129>>>>>>>>>>>>>        set psLeft         to sLeft
79130>>>>>>>>>>>>>        set psRight        to sRight
79131>>>>>>>>>>>>>        Set piPoints       to iDigits
79132>>>>>>>>>>>>>     End_procedure
